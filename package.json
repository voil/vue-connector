{
  "name": "@webonweb/vue-connector",
  "type": "module",
  "version": "0.2.2",
  "description": "This Vue library enables the creation of interactive workflow transitions and visual IVR diagrams. Featuring an intuitive drag-and-drop interface and ready-made components, developers can quickly design and integrate complex business processes and call-routing scenarios into their applications. Its flexibility and ease of configuration make it an ideal solution for implementing dynamic communication systems and workflow automation",
  "author": "webonweb",
  "license": "MIT",
  "homepage": "https://github.com/voil/vue-connector",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voil/vue-connector.git"
  },
  "bugs": {
    "url": "https://github.com/voil/vue-connector/issues"
  },
  "keywords": [
    "vue",
    "connector",
    "flow",
    "typescript",
    "vue3",
    "state"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.cjs",
      "import": "./dist/index.js"
    },
    "./style.css": {
      "import": "./dist/index.css"
    },
    "./types": {
      "import": "./dist/types.d.ts"
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./LICENSE"
  ],
  "scripts": {
    "build": "bun build-only",
    "build-only": "vite build",
    "watch": "vite build --watch",
    "type-gen": "vue-tsc --declaration  --emitDeclarationOnly",
    "lint": "eslint --ext .js,.vue src",
    "test": "vitest",
    "pub:release": "npm publish --access public"
  },
  "peerDependencies": {
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/bun": "^1.2.7",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/shared": "^3.5.13",
    "@vue/tsconfig": "^0.7.0",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "eslint-config-prettier": "^10.1.1",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-vue": "^10.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.86.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vite": "^4.5.10",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-glob": "^0.3.2",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  }
}
